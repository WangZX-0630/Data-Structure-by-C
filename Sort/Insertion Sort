#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void InsertionSort(int A[], int N) {
    int tmp, j;
    for(int i = 1; i < N; ++i) {
        tmp = A[i];
        for(j = i; j > 0 && A[j - 1] > tmp; --j) {
            A[j] = A[j - 1];
        }
        A[j] = tmp;
    }
}

void test(int array[], int N) {
    InsertionSort(array, N);
    /*for(int i = 0; i < N; ++i) {
        printf("%d,",array[i]);
    }*/
}

int main()
{
    int N = 100000;
    int array[N];
    clock_t start, finish;
    srand((unsigned)time(NULL));
    for(int i = 0; i < N; ++i) {
        array[i] = rand() % (10 * N);
    }
    start = clock();
    test(array, N);
    finish = clock();
    printf("\nProcessing time: %1f ms\n", (double)(finish - start) / 1000);
    
    return 0;
}
/* On my computer, it cost about 4.85s to sort 100000 number with BubbleSort */
